{
  "max_open_trades": 3,
  "stake_currency": "USDT",
  "stake_amount": "unlimited",
  "tradable_balance_ratio": 0.05,
  "fiat_display_currency": "USD",
  "dry_run": true,
  "dry_run_wallet": 1000,
  "cancel_open_orders_on_exit": false,
  
  "trading_mode": "spot",
  "margin_mode": "",
  
  "unfilledtimeout": {
    "entry": 10,
    "exit": 10,
    "exit_timeout_count": 0,
    "unit": "minutes"
  },
  
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },
  
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1
  },
  
  "exchange": {
    "name": "binance",
    "key": "${BINANCE_API_KEY}",
    "secret": "${BINANCE_API_SECRET}",
    "ccxt_config": {
      "enableRateLimit": true,
      "urls": {
        "api": {
          "public": "https://testnet.binance.vision/api",
          "private": "https://testnet.binance.vision/api"
        }
      }
    },
    "ccxt_async_config": {
      "enableRateLimit": true,
      "urls": {
        "api": {
          "public": "https://testnet.binance.vision/api",
          "private": "https://testnet.binance.vision/api"
        }
      }
    },
    "pair_whitelist": [],
    "pair_blacklist": [
      "BNB/.*"
    ]
  },
  
  "pairlists": [
    {
      "method": "VolumePairList",
      "number_assets": 20,
      "sort_key": "quoteVolume",
      "min_value": 0,
      "refresh_period": 1800
    },
    {
      "method": "AgeFilter",
      "min_days_listed": 7
    },
    {
      "method": "PrecisionFilter"
    },
    {
      "method": "PriceFilter",
      "low_price_ratio": 0.01,
      "min_price": 0.00001,
      "max_price": 100000
    },
    {
      "method": "SpreadFilter",
      "max_spread_ratio": 0.005
    },
    {
      "method": "VolatilityFilter",
      "lookback_days": 3,
      "min_volatility": 0.01,
      "max_volatility": 0.75,
      "refresh_period": 1800
    },
    {
      "method": "ShuffleFilter",
      "seed": 42
    }
  ],
  
  "edge": {
    "enabled": false
  },
  
  "telegram": {
    "enabled": true,
    "token": "${TELEGRAM_BOT_TOKEN}",
    "chat_id": "${TELEGRAM_CHAT_ID}",
    "notification_settings": {
      "status": "on",
      "warning": "on",
      "startup": "on",
      "entry": "on",
      "entry_fill": "on",
      "entry_cancel": "on",
      "exit": "on",
      "exit_fill": "on",
      "exit_cancel": "on",
      "protection_trigger": "on",
      "protection_trigger_global": "on",
      "show_candle": "on",
      "strategy_msg": "on"
    },
    "reload": true,
    "balance_dust_level": 0.01
  },
  
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "verbosity": "error",
    "enable_openapi": false,
    "jwt_secret_key": "${FREQTRADE_API_PASSWORD}",
    "CORS_origins": ["*"],
    "username": "${FREQTRADE_API_USERNAME}",
    "password": "${FREQTRADE_API_PASSWORD}"
  },
  
  "bot_name": "ML_Scalping_Bot",
  "initial_state": "running",
  "force_entry_enable": false,
  "internals": {
    "process_throttle_secs": 5
  },
  
  "freqai": {
    "enabled": true,
    "purge_old_models": true,
    "train_period_days": 30,
    "backtest_period_days": 7,
    "identifier": "ml_scalping_model",
    "feature_parameters": {
      "include_timeframes": ["5m", "15m", "1h"],
      "include_corr_pairlist": [
        "BTC/USDT",
        "ETH/USDT"
      ],
      "label_period_candles": 20,
      "include_shifted_candles": 2,
      "DI_threshold": 0.9,
      "weight_factor": 0.9,
      "principal_component_analysis": false,
      "use_SVM_to_remove_outliers": true,
      "stratify_training_data": 0,
      "indicator_periods_candles": [10, 20, 50]
    },
    "data_split_parameters": {
      "test_size": 0.33,
      "shuffle": false
    },
    "model_training_parameters": {
      "n_estimators": 1000,
      "learning_rate": 0.02,
      "max_depth": 8,
      "num_leaves": 64,
      "verbosity": -1,
      "random_state": 42
    }
  }
}
